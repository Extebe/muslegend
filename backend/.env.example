# Port du serveur backend
PORT=3001

# Environnement (development | production)
NODE_ENV=development

# CORS Origin (mettre votre domaine en production)
CORS_ORIGIN=*

# Configuration du jeu
DEFAULT_WIN_SCORE=40
MAX_PLAYERS=4

# Timeouts (en millisecondes)
RECONNECT_TIMEOUT=60000
ACTION_TIMEOUT=45000

# Valeurs de mises autorisées (séparées par des virgules)
BET_VALUES=1,2,3,4,5

# Nombre maximum de relances (KANTA)
MAX_KANTA=1